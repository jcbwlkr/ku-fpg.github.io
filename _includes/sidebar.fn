{% if include.level > 0 %}<ul>{% endif %}
{% for item in include.param %}
{% if include.level > 0 %}<li>{% else %}<h4>{% endif %}
  <a href="{{item.href}}">{{item.name}}</a>
{% if include.level > 0 %}</li>{% else %}</h4>{% endif %}
  {% if item.sublinks %}
      {% assign level = include.level | plus: 1 %}
      {% include sidebar.fn param=item.sublinks level=level %}
  {% endif %}
{% endfor %}
{% if include.level > 0 %}</ul>{% endif %}
